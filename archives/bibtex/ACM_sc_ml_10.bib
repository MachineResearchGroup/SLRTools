series = {ICIT 2019}
}

@inproceedings{10.1145/3428502.3428506,
author = {Ches\~{n}evar, Carlos Iv\'{a}n and Gonz\'{a}lez, Mar\'{\i}a Paula and Maguitman, Ana and Estevez, Elsa},
