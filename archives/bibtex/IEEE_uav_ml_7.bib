@INPROCEEDINGS{9569295,
author={Benmad, Ilham and Driouch, Elmahdi and Kardouchi, Mustapha},
booktitle={2021 IEEE 32nd Annual International Symposium on Personal, Indoor and Mobile Radio Communications (PIMRC)}, title={Data Collection in UAV-Assisted Wireless Sensor Networks Powered by Harvested Energy},
year={2021},
