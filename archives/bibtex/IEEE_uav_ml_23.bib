@ARTICLE{9354009,
author={Zeng, Yong and Xu, Xiaoli and Jin, Shi and Zhang, Rui},
journal={IEEE Transactions on Wireless Communications}, title={Simultaneous Navigation and Radio Mapping for Cellular-Connected UAV With Deep Reinforcement Learning},
year={2021},
