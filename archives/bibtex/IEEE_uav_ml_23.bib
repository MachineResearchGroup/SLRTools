@INPROCEEDINGS{9331177,
author={Saeed, Meer Shadman},
booktitle={2021 2nd International Conference on Robotics, Electrical and Signal Processing Techniques (ICREST)}, title={Unmanned Aerial Vehicle for Automatic Detection of Concrete Crack using Deep Learning},
