@inproceedings{10.1145/3428502.3428503,
author = {Udoh, Emmanuel Sebastian},
title = {Is the Data Fair? An Assessment of the Data Quality of Algorithmic Policing Systems},
year = {2020},
isbn = {9781450376747},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi-org.ez294.periodicos.capes.gov.br/10.1145/3428502.3428503},
doi = {10.1145/3428502.3428503},
abstract = {With the growing ubiquity of algorithmic governance systems in the public sector, including policing, potentially life-changing decisions are daily being made on an automated basis, potentially raising several fairness and inequity concerns and issues. However, while much of the algorithmic policing scholarship focuses on the potential risks of algorithmic bias, profiling and systematic exclusion or disenfranchisement of some demographics, the question of data quality as a potentially vitiating factor tends to be neglected. This study seeks to problematize the critical role of data quality in the evaluation of the outcomes of algorithmic selection or prediction systems using publicly available data from a Police Department as an example. Following traditional frameworks for handling missing data in analysis, a missingness analysis on the dataset shows a high imbalance and therefore a potentially biased prediction model. The paper argues that data quality is as crucial a factor as the algorithm itself in potentially colouring the outcomes of the algorithmic decisions, thus highlighting the importance of data quality strategies as a safeguard against potential inequity.},
booktitle = {Proceedings of the 13th International Conference on Theory and Practice of Electronic Governance},
pages = {1–7},
numpages = {7},
keywords = {predictive policing, Algorithmic governance, data quality, missingness analysis, algorithmic policing, missing data},
location = {Athens, Greece},
series = {ICEGOV 2020}
}

@article{10.1145/3273982.3273995,
