@INPROCEEDINGS{8057438,
author={Alihodzic, Adis},
booktitle={2016 3rd International Conference on Soft Computing Machine Intelligence (ISCMI)}, title={Fireworks Algorithm with New Feasibility-Rules in Solving UAV Path Planning},
year={2016},
volume={},
