@INPROCEEDINGS{8437104,
author={P., Aparna and Hebbar, Ramachandra and M.P., Harshita and Sounder, Harshitha and K., Nandkishore and Vinod, P.V.},
booktitle={2018 International Conference on Design Innovations for 3Cs Compute Communicate Control (ICDI3C)}, title={CNN Based Technique for Automatic Tree Counting Using Very High Resolution Data},
year={2018},
