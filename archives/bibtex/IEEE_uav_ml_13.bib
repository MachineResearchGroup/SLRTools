@INPROCEEDINGS{8898156,
author={Xia, Xue and Koeva, Mila and Persello, Claudio},
booktitle={IGARSS 2019 - 2019 IEEE International Geoscience and Remote Sensing Symposium}, title={Extracting Cadastral Boundaries from UAV Images Using Fully Convolutional Networks},
