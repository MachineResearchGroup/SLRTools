@ARTICLE{8726071,
author={Hu, Long and Tian, Yuanwen and Yang, Jun and Taleb, Tarik and Xiang, Lin and Hao, Yixue},
journal={IEEE Network}, title={Ready Player One: UAV-Clustering-Based Multi-Task Offloading for Vehicular VR/AR Gaming},
