@INPROCEEDINGS{8647926,
author={Jindal, Anish and Aujla, Gagangeet Singh and Kumar, Neeraj and Prodan, Radu and Obaidat, Mohammad S.},
booktitle={2018 IEEE Global Communications Conference (GLOBECOM)}, title={DRUMS: Demand Response Management in a Smart City Using Deep Learning and SVR},
year={2018},
volume={},
