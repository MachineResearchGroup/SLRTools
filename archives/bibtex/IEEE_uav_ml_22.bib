month={Sep.},}
@INPROCEEDINGS{7729174,
author={Bejiga, Mesay Belete and Zeggada, Abdallah and Melgani, Farid},
booktitle={2016 IEEE International Geoscience and Remote Sensing Symposium (IGARSS)}, title={Convolutional neural networks for near real-time object detection from UAV imagery in avalanche search and rescue operations},
year={2016},
