@INPROCEEDINGS{9145458,
author={Chang, Zheng and Guo, Wenlong and Guo, Xijuan and Ristaniemi, Tapani},
booktitle={2020 IEEE International Conference on Communications Workshops (ICC Workshops)}, title={Machine Learning-Based Resource Allocation for Multi-UAV Communications System},
year={2020},
volume={},
number={},
