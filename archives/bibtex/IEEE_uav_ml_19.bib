@INPROCEEDINGS{9120592,
author={Zhang, Yu and Zhuang, Zirui and Gao, Feifei and Wang, Jingyu and Han, Zhu},
booktitle={2020 IEEE Wireless Communications and Networking Conference (WCNC)}, title={Multi-Agent Deep Reinforcement Learning for Secure UAV Communications},
year={2020},
volume={},
