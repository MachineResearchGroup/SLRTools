@INPROCEEDINGS{9321600,
author={Chen, Zhong and Yeh, Shihyuan and Chamberland, Jean-Francois and Huff, Gregory H.},
booktitle={2020 IEEE USNC-CNC-URSI North American Radio Science Meeting (Joint with AP-S Symposium)}, title={Impact of Position Errors on Synthetic Aperture DOA Convergence Based on Swarming UAV s},
year={2020},
volume={},
