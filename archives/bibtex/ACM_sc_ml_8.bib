url = {https://doi-org.ez294.periodicos.capes.gov.br/10.1145/3377170.3377275},
doi = {10.1145/3377170.3377275},
abstract = {This paper proposed a new algorithm based on kernel discriminant analysis (KDA) for classification of magnetic resonance (MR) brain images. First, the original MR brain image is preprocessed to extract the grey matter density of the whole brain as the imaging features. To reduce the feature dimension, t-test is then used to select the most relevant features for classification. Finally, we apply the KDA to classify the selected features into two classes. The proposed algorithm is evaluated using 427 T1-weighted MR brain images from ADNI which includes 198 subjects of Alzheimer disease (AD) and 229 normal controls (NC). Experimental results show that our proposed algorithm achieves the classification accuracy of 88.07% for classification of AD vs. NC, which demonstrates the promising performance.},
booktitle = {Proceedings of the 2019 7th International Conference on Information Technology: IoT and Smart City},
pages = {449–452},
numpages = {4},
keywords = {Kernel discriminant analysis, AD diagnosis, MR brain images, Image classification},
