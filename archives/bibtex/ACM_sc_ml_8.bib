@inbook{10.1145/3483207.3483225,
author = {Cui, Chengkun and Wang, Chen and Wen, Cong and Fang, Xiaoyin and Deng, Yuanyuan},
title = {Intelligent News Recommendation System for Automobile Based on Bio-Sensing and Physiological Computing},
year = {2021},
isbn = {9781450390170},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi-org.ez294.periodicos.capes.gov.br/10.1145/3483207.3483225},
abstract = {With the development and application of technologies such as big data and artificial intelligence, the automotive industry is confronted with unprecedented challenges. New technologies are coming into the spotlight while potentials for transformation are growing under the surface, leading to the omnipresence of challenges across various fields. Under such circumstances, media and auto companies are both aware of an imminent change. The smart ring developed by our team can be applied to the driver fatigue detection system in industrial production— which helps media companies to access the vehicle networking system by monitoring the fatigue status of drivers, as well as their physiological changes when listening to the news. The vehicle networking system recommends personalized contents for users based on their portraits, using algorithm not only considers their interests and browsing behaviors, but also takes physiological and psychological responses of vehicle users when listening to the news as a key dimension based on the smart ring; therefore, it enables integration of online behavioral and offline physiological data, as such making up for where traditional algorithms fall short. Through intelligent interaction of media contents and human physiological signals, it changes the existing process of news production, manufacturing, distribution, collection, and feedback, and thus will become a model of the future smart recommendation system for in-vehicle infotainment. At present, research findings have been applied to two vehicle types of an auto company, which will come into the market in the first half of this year.},
booktitle = {2021 4th International Conference on Signal Processing and Machine Learning},
pages = {107–111},
numpages = {5}
}

@article{10.1145/3404890,
author = {Feng, Jun and Yang, Laurence T. and Zhu, Yuxiang and Gati, Nicholaus J. and Mo, Yijun},
title = {Blockchain-Enabled Tensor-Based Conditional Deep Convolutional GAN for Cyber-Physical-Social Systems},
year = {2021},
issue_date = {June 2021},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {21},
number = {2},
issn = {1533-5399},
url = {https://doi-org.ez294.periodicos.capes.gov.br/10.1145/3404890},
doi = {10.1145/3404890},
abstract = {Deep learning techniques have shown significant success in cyber-physical-social systems (CPSS). As an instance of deep learning models, generative adversarial nets (GAN) model enables powerful and flexible image augmentation, image generation, and classification, thus can be applied to real-world CPSS settings. GAN model training needs a large collection of cyber-physical-social data originating from various CPSS devices. Numerous prevailing GAN models depend on a tacit assumption that several cyber-physical-social data providers present a reliable source to collect training data, which is seldom the case in real CPSS. The existing GAN models also fail to consider multi-dimensional latent structure. In our work, we put forward a novel blockchain-enabled tensor-based conditional deep convolutional GAN (TCDC-GAN) model for cyber-physical-social systems. The blockchain is employed to develop a decentralized and reliable cyber-physical-social data-sharing platform between numerous cyber-physical-social data providers, such that the training data and the model are documented on a ledger that is distributed. Furthermore, a tensor-based generator and a tensor-based discriminator are well designed by employing the tensor model. The results of extensive simulation experiments show the efficacy of the proposed TCDC-GAN model. Compared with the state-of-the-art models, our model gains superior estimation performance.},
journal = {ACM Trans. Internet Technol.},
month = {jun},
articleno = {41},
numpages = {17},
keywords = {generative adversarial network, Cyber-physical-social systems, tensor, deep learning, blockchain}
}

