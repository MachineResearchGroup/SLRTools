@ARTICLE{8698764,
author={Pan, Xiuqin and Zhou, Wangsheng and Lu, Yong and Sun, Na},
journal={IEEE Access}, title={Prediction of Network Traffic of Smart Cities Based on DE-BP Neural Network},
year={2019},
