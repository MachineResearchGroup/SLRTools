@ARTICLE{8952612,
author={Hurst, William and Curbelo Montañez, Casimiro Aday and Shone, Nathan and Al-Jumeily, Dhiya},
journal={IEEE Access}, title={An Ensemble Detection Model Using Multinomial Classification of Stochastic Gas Smart Meter Data to Improve Wellbeing Monitoring in Smart Cities},
