@ARTICLE{9241477,
author={Sinaeepourfard, Amir and Krogstie, John and Sengupta, Souvik},
journal={IEEE Internet of Things Magazine}, title={Distributed-to-Centralized Data Management: A New Sense of Large-Scale ICT Management of Smart City IoT Networks},
year={2020},
volume={3},
number={3},
pages={76-82},
abstract={Modern cities are equipped with various information and communications technology (ICT) resources including Internet of Things (IoT) devices, computing platforms, and data storage media. Data are one of the most valuable ICT resources in smart cities. Data management strategies play a vital role in managing the requirements of user and business models in a city. Using the benefits of data makes an agile, creative, and smart city via the widespread use of appropriate city services. Centralized and distributed-to-centralized data management (D2CDM) architectures are recommended to organize the large-scale produced city data in smart city networks, including physical data resources (e.g., sensor data) and nonphysical data sources (e.g., city consumer personal databases). In this article, we explain two different ICT technology management solutions for smart city networks in which the ICT resources in a city can be managed and delivered: centralized and distributed-to-centralized. We also describe two main strategies for data management in smart cities, and the advantages of D2C-DM are discussed based on two ongoing case studies. We also mention there is much room for future works and development of this study, such as developing software services in smart cities based on edge-to-cloud orchestration and enhancing the effectiveness of machine learning (ML) and artificial intelligence (AI) techniques through multilevel ICT architecture.},
keywords={Smart cities;Computer architecture;Distributed databases;Cloud computing;Internet of Things;Information and communications technology},
doi={10.1109/IOTM.0001.1900038},
ISSN={2576-3199},
month={Sep.},}
@INPROCEEDINGS{8723846,
author={Reda, Mali and Mountassir, Fouad and Mohamed, Bousmah},
booktitle={2019 International Conference on Wireless Technologies, Embedded and Intelligent Systems (WITS)}, title={Introduction to Coordinated Deep Agents for Traffic Signal},
year={2019},
volume={},
number={},
pages={1-6},
abstract={Traffic Signal control optimization is a key element is future smart cities. We present, in this paper, an optimized agent, based on Reinforcement Learning, with Double Deep Q-Learning. Since intersections influence each other, cooperation between agents will be key in our study. For state representation, we use real-time data, that can be collected by a smart camera, as vehicle position and speed. The rewards function will consider the waiting time of drivers, which has the advantage of better representing the case of congestions in large agglomerations. In our network, we will use Convolutional Layers to extract features that will help our model to learn the best actions to take, in every state. This model will be used to solve convergence and oscillation problems that commonly appears in multi-agent context with Neuronal Networks. A future article will present the details of the implementation and the results obtained and will make a comparison between our model and other state-of-art solutions used in the Traffic Signal Control.},
keywords={Reinforcement learning;Feature extraction;Roads;Real-time systems;Oscillators;Sociology;Statistics;Trafc Signal Control;Deep Reinforcement Learning},
