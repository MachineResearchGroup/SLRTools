@ARTICLE{9099242,
author={Wu, Huaming and Zhang, Ziru and Guan, Chang and Wolter, Katinka and Xu, Minxian},
journal={IEEE Internet of Things Journal}, title={Collaborate Edge and Cloud Computing With Distributed Deep Learning for Smart City Internet of Things},
year={2020},
