@INPROCEEDINGS{9322104,
author={Zhang, Chenhan and Zhang, Shuyu and Yu, James J. Q. and Yu, Shui},
booktitle={GLOBECOM 2020 - 2020 IEEE Global Communications Conference}, title={An Enhanced Motif Graph Clustering-Based Deep Learning Approach for Traffic Forecasting},
year={2020},
volume={},
