@INPROCEEDINGS{9175536,
author={Jin, Jingyi and Zhang, Tong and Cone, Kevin and Xu, Beryl},
booktitle={2020 IEEE Conference on Multimedia Information Processing and Retrieval (MIPR)}, title={An Innovative Acceleration Model for Media Analytics},
