@ARTICLE{9177317,
author={Liang, Hongbin and Zhang, Xiaohui and Hong, Xintao and Zhang, Zongyuan and Li, Mushu and Hu, Guangdi and Hou, Fen},
journal={IEEE Transactions on Industrial Informatics}, title={Reinforcement Learning Enabled Dynamic Resource Allocation in the Internet of Vehicles},
year={2021},
