@INPROCEEDINGS{8358523,
author={Aladi, Harsha B and Saha, Snehanshu and Kurian, Abu and Basu, Aparna},
booktitle={2017 International Conference On Smart Technologies For Smart Nation (SmartTechCon)}, title={Predictive analytics for safer smart cities},
year={2017},
