@ARTICLE{9184996,
author={Hassaballah, M. and Kenk, Mourad A. and Muhammad, Khan and Minaee, Shervin},
journal={IEEE Transactions on Intelligent Transportation Systems}, title={Vehicle Detection and Tracking in Adverse Weather Using a Deep Learning Framework},
year={2021},
