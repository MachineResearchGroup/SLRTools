@INPROCEEDINGS{8029849,
author={Wang, Yujie and Fan, Xiaoliang and Liu, Xiao and Zheng, Chuanpan and Chen, Longbiao and Wang, Cheng and Li, Jonathan},
booktitle={2017 IEEE International Conference on Web Services (ICWS)}, title={Unlicensed Taxis Detection Service Based on Large-Scale Vehicles Mobility Data},
year={2017},
volume={},
