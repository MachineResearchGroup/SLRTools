@INPROCEEDINGS{9597700,
author={M., Palak and Revati, G. and Hossain, Md Alamgir and Sheikh, A.},
booktitle={2021 31st Australasian Universities Power Engineering Conference (AUPEC)}, title={Deep Learning models for Smart Building Load Profile Prediction},
year={2021},
volume={},
