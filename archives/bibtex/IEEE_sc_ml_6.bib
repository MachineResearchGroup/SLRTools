@INPROCEEDINGS{8939091,
author={Raja, Gunasekaran and Ganapathisubramaniyan, Aishwarya and Selvakumar, Madhumitha Sri and Ayyarappan, Thiruveni and Mahadevan, Karthikeyan},
booktitle={2018 Tenth International Conference on Advanced Computing (ICoAC)}, title={Cognitive Intelligent Transportation System for Smart Cities},
year={2018},
volume={},
number={},
pages={146-152},
abstract={Recent advancement in hardware, software and communication technologies have culminated in improvisation of safety aspects in vehicular communication networks. Vehicular Ad hoc NETworks (VANETs) is used to establish network among the vehicles which paves way for Intelligent Transportation System. In spite of myriad of benefits in VANETs, there exists enormous amount of concerns such as establishing privacy, safety monitoring, reliable connectivity, high bandwidth, less delay with guaranteed Quality of Service (QoS). Establishing security services in the VANET is the key to make any application successful in the safe driving condition. As part of intelligence in cognitive systems, Machine Learning algorithms make it possible for constantly mine data and acquire knowledge through advanced analytics. We use clustering, a machine learning algorithm for clustering the vehicles at intersection and grouping the messages in each cluster and filtering the redundant message. Naive Bayes algorithm is applied over the filtered message for classifying the messages and assigning priority before transmission. Further, our proposed Cognitive-Intelligent Transportation System (C-ITS) model provides efficient channel utilization and the accuracy is improved compare to the existing methods of classification. Finally, performance and accuracy of proposed approach are evaluated.},
keywords={Vehicular ad hoc networks;Clustering algorithms;Safety;Bandwidth;Delays;Broadcasting;Sensors;Vehicular Ad hoc NETworks;Machine Learning;Intelligent Transportation System;Clustering;Naive Bayes},
