@INPROCEEDINGS{7554221,
author={Zhao, Chunhui and Zhou, Yihui and Wei, Yanyan and Jiang, Shan and Yu, Changbin},
booktitle={2016 35th Chinese Control Conference (CCC)}, title={Visual saliency landmark detection algorithm based on DNN feature extraction},
year={2016},
volume={},
number={},
