@ARTICLE{9469894,
author={Luo, Luqing and Tang, Lulu and Liu, Rui and Zhang, Xiaoli and Yang, Zhi-Xin},
journal={IEEE Sensors Journal}, title={Multi-Modality Learning for Non-Rigid 3D Shape Retrieval via Structured Sparsity Regularizations},
year={2021},
