@INPROCEEDINGS{8551322,
author={Satti, Muhammad Bilal and Ali, Nouman and Asif, Mian Mohammad Arsalan and Abid, Abdul Hafeez and Hasan, Ammar},
booktitle={2018 15th International Conference on Smart Cities: Improving Quality of Life Using ICT IoT (HONET-ICT)}, title={A Low-Cost Smart Load Enabler Device},
year={2018},
