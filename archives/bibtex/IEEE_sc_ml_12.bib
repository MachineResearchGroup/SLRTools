@INPROCEEDINGS{6890424,
author={Xiang Chen and Jie-Bin Xu and Wei-Qiang Guo},
booktitle={2013 International Conference on Machine Learning and Cybernetics}, title={The research about video surveillance platform based on cloud computing},
year={2013},
