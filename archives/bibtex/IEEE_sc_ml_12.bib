@INPROCEEDINGS{9562953,
author={Almalki, Khalid J and Mohzary, Muhammad and Choi, Baek-Young and Song, Sejun and Chen, Yu},
booktitle={2021 IEEE International Smart Cities Conference (ISC2)}, title={Mosaic: Modeling Safety Index in Crowd by Detecting Face Masks against COVID-19 and Beyond},
year={2021},
volume={},
