@INPROCEEDINGS{9322280,
author={Chen, Tianran and Zhang, Yongzheng and Tuo, Yupeng and Wang, Weiguang},
booktitle={GLOBECOM 2020 - 2020 IEEE Global Communications Conference}, title={CgNet: Predicting Urban Congregations from Spatio-Temporal Data using Deep Neural Networks},
year={2020},
volume={},
number={},
pages={1-7},
abstract={Predicting urban congregations can help in monitoring a variety of unusual group events, which is of great importance to public safety and traffic management in smart cities. However, it is very challenging because of complicated spatio-temporal correlations. In this article, we propose a deep neural network-based model, entitled CgNet, for urban congregations prediction. Firstly, we design three types of flows to present dependencies between regions among different timestamps to model the mobility of individuals. Secondly, CgNet utilizes four components, including spatial feature extraction, temporal feature extraction, external factors fusion and congregation feature fusion to collaboratively predict congregations. The combination of these components is capable of not only capturing the spatial and temporal correlations simultaneously, but also learning the essential relationships between three flows and congregations in each region at different stages. Finally, we evaluated the effectiveness of CgNet with extensive experimental study on real taxi trajectory data. The results demonstrate the advantages of our model beyond several baselines.},
keywords={Feature extraction;Correlation;Market research;Trajectory;Predictive models;Neural networks;Smart cities;urban congregations prediction;smart cities application;deep neural network;spatio-temporal data},
doi={10.1109/GLOBECOM42002.2020.9322280},
