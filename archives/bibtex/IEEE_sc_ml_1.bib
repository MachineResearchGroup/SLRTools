@INPROCEEDINGS{8343172,
author={Munir, Md. Shirajum and Abedin, Sarder Fakhrul and Alam, Md. Golam Rabiul and Tran, Nguyen H. and Hong, Choong Seon},
booktitle={2018 International Conference on Information Networking (ICOIN)}, title={Intelligent service fulfillment for software defined networks in smart city},
year={2018},
volume={},
