@INPROCEEDINGS{7463827,
author={Fu, Xingang and Lu, Jiang and Zhang, Ting and Bonair, Chadwell and Coats, Marvin L.},
booktitle={2015 IEEE International Conference on Smart City/SocialCom/SustainCom (SmartCity)}, title={Wavelet Enhanced Image Preprocessing and Neural Networks for Hand Gesture Recognition},
year={2015},
volume={},
