@INPROCEEDINGS{9545879,
author={Sanad, A. J. and Hammad, M.},
booktitle={3rd Smart Cities Symposium (SCS 2020)}, title={Predicting cloud spot price using machine learning},
year={2020},
volume={2020},
number={},
pages={572-578},
abstract={Cost reduction is considered one of the main benefits that are offered by cloud computing technology. Spot leasing is the cheapest leasing scheme for cloud computing resources, where the customers are trading the low service reliability to get a cheaper leasing price. The spot leasing is subjected to dynamic price changing due to the supply and demand fluctuations. Price changing is forcing the cloud service platform to reclaim the resources from the customers who offer lower prices and assign them to customers who pay higher prices. This issue increases the chance of getting the service interrupted, which reduces service reliability. Price changing effects can be mitigated by using price prediction. This paper tries to measure the feasibility of applying several machine learning algorithms on data collected from Amazon AWS. Four algorithms have been tested including linear regression, multilayer perceptron MLP, SMO regression and the REPTree. The results show that using the prediction algorithms are giving a high level of prediction accuracy. Furthermore, the SMO regression gave the best results for low fluctuation rates, while the REPTree was better with high fluctuation rates.},
