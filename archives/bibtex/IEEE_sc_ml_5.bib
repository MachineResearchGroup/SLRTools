@ARTICLE{9253530,
author={Zheng, Ye and Guo, Renzhong and Ma, Ding and Zhao, Zhigang and Li, Xiaoming},
journal={IEEE Access}, title={A Novel Approach to Coordinating Green Wave System With Adaptation Evolutionary Strategy},
year={2020},
