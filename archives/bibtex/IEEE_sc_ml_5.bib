@INPROCEEDINGS{9500825,
author={Pathak, Aditya Kumar and Saguna, Saguna and Mitra, Karan and Åhlund, Christer},
booktitle={ICC 2021 - IEEE International Conference on Communications}, title={Anomaly Detection using Machine Learning to Discover Sensor Tampering in IoT Systems},
year={2021},
