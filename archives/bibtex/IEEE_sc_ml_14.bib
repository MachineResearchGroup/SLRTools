@ARTICLE{9096320,
author={Lopez-Ballester, Jesus and Pastor-Aparicio, Adolfo and Felici-Castell, Santiago and Segura-Garcia, Jaume and Cobos, Maximo},
journal={IEEE Sensors Journal}, title={Enabling Real-Time Computation of Psycho-Acoustic Parameters in Acoustic Sensors Using Convolutional Neural Networks},
