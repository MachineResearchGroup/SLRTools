@INPROCEEDINGS{7575868,
author={Ramparany, Fano and Thalgott, Marceau and Bolle, Sébastien and Martin, Serge},
booktitle={2016 IEEE 4th International Conference on Future Internet of Things and Cloud (FiCloud)}, title={Exploiting Data Analytics for Home Automation Services},
year={2016},
volume={},
