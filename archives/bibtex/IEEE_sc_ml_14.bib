author={Chen, Tian-Xiang and Hsiao, Rong-Shue and Kao, Chun-Hao and Lin, Ding-Bing and Yang, Bo-Ru},
booktitle={2018 IEEE SmartWorld, Ubiquitous Intelligence Computing, Advanced Trusted Computing, Scalable Computing Communications, Cloud Big Data Computing, Internet of People and Smart City Innovation (SmartWorld/SCALCOM/UIC/ATC/CBDCom/IOP/SCI)}, title={Bed-Exit Prediction Based on 3D Convolutional Neural Network},
