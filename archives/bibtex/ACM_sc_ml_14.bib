@article{10.1145/3231541.3231546,
author = {Zang, Andi and Chen, Xin and Trajcevski, Goce},
title = {High Definition Maps in Urban Context},
year = {2018},
issue_date = {March 2018},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
volume = {10},
number = {1},
url = {https://doi-org.ez294.periodicos.capes.gov.br/10.1145/3231541.3231546},
doi = {10.1145/3231541.3231546},
abstract = {Part of the challenges in the quest for smart cities is to enable effective navigation for different types of mobile users: from pedestrians, through drivers, to autonomous vehicles. While the data sources to facilitate such tasks abound, one of the pressing problems is how to enable efficient management and download of the data needed to populate the screens of devices with the appropriate visualization.In this paper, we present an overview of the different issues and the main features of the existing technologies that, in one way or another, could be used as foundations for effective solution for generating quality maps. We also discuss the possible approaches for addressing such issues in the context of accurate self-localization of vehicles.},
journal = {SIGSPATIAL Special},
