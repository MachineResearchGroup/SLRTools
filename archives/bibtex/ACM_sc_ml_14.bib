@inproceedings{10.1145/3277593.3277605,
author = {Grammatopoulou, Maria and Kanellopoulos, Aris and Vamvoudakis, Kyriakos G.},
title = {A Multi-Step and Resilient Predictive Q-Learning Algorithm for IoT: A Case Study in Water Supply Networks},
