@INPROCEEDINGS{9060336,
author={Liao, Jun and Liu, Dandan and Hu, Junfeng and Li, Yande and Khan, Aamir and Liu, Li and Xiong, Qingyu and Wen, Junhao},
booktitle={2019 IEEE SmartWorld, Ubiquitous Intelligence Computing, Advanced Trusted Computing, Scalable Computing Communications, Cloud Big Data Computing, Internet of People and Smart City Innovation (SmartWorld/SCALCOM/UIC/ATC/CBDCom/IOP/SCI)}, title={Individual Risk Prediction of Gastric Cancer Using Fully Connected Neural Network with Weighted Neighborhood Feature},
year={2019},
volume={},
number={},
pages={1219-1224},
abstract={Gastric cancer is one of the common malignant tumor diseases, with its incidence increasing year after year. The primary issue of preventing gastric cancer is to assess its risk level. Most of the current approaches merely contain histological and demographic information for cancer risk assessment. However, the factors of patient-based information, such as family history, diet, high-risk habits and exposure to environmental carcinogens are often not considered in cancer prediction. To address the issue, we present a fully connected neural network method with weighted neighborhood feature (WNF-FCNN) to improve the accuracy of gastric cancer risk prediction based on patient-based information. We first adopt two feature selection schemes to choose weighted neighborhood feature from the 121 raw medical attributes about gastric cancer. Then, a four-layer FCNN is constructed to model the risk level of gastric cancer with Adam optimization algorithm being used to speed up the convergence during training stages. Empirical evaluations on a real hospital cancer medical dataset containing 50,611 records demonstrate the competitiveness of our approach. The results show that our approach is viable to assess individual gastric cancer risk using patient-based information, with the accuracy rate of 96.2%. Our experimental results also show that the neural network-based approach outperforms other competing approaches in terms of prediction accuracy.},
keywords={Cancer;Feature extraction;Predictive models;Big Data;Software engineering;Neural networks;Training;Gastric Cancer;weighted neighborhood feature;feature selection;full connected neural network;risk prediction},
doi={10.1109/SmartWorld-UIC-ATC-SCALCOM-IOP-SCI.2019.00227},
ISSN={},
month={Aug},}
@INPROCEEDINGS{8855588,
author={Xu, Shuliang and Liu, Shenglan and Feng, Lin},
booktitle={2019 IEEE 21st International Conference on High Performance Computing and Communications; IEEE 17th International Conference on Smart City; IEEE 5th International Conference on Data Science and Systems (HPCC/SmartCity/DSS)}, title={Neighborhood Graph Embedding for Nodes Clustering of Social Network},
year={2019},
volume={},
number={},
pages={718-725},
abstract={Graph embedding is an important dimension reduction method for high-dimensional data. In this paper, a neighborhood graph embedding algorithm is proposed and it is applied in data clustering. Different from the traditional graph embedding algorithms, a dependence degree of node is defined and it represents the dependence of two nodes; the adjacency matrix of graph is determined by dependence degree. Then a new graph embedding is proposed. After transformation matrix is solved, the weight of each attribute can also be determined from transformation matrix. Finally, the data is partitioned into clusters by clustering algorithm with weighting distance. The proposed algorithm and comparison algorithms are executed on the real social network data sets. The experimental results show that the proposed algorithm out performances the comparison algorithms and it proves that the proposed algorithm is effective for data clustering in social network.},
keywords={Clustering algorithms;Social networking (online);Partitioning algorithms;Artificial neural networks;Complex networks;Conferences;Conductivity;Neighborhood;social network;clustering analysis;graph embedding;dimension reduction},
doi={10.1109/HPCC/SmartCity/DSS.2019.00107},
