@ARTICLE{9208765,
author={Hussain, Tanveer and Muhammad, Khan and Ullah, Amin and Ser, Javier Del and Gandomi, Amir H. and Sajjad, Muhammad and Baik, Sung Wook and de Albuquerque, Victor Hugo C.},
journal={IEEE Internet of Things Journal}, title={Multiview Summarization and Activity Recognition Meet Edge Computing in IoT Environments},
year={2021},
