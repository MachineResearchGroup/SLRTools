@INPROCEEDINGS{8918913,
author={Cohen-Hadria, Alice and Cartwright, Mark and McFee, Brian and Bello, Juan Pablo},
booktitle={2019 IEEE 29th International Workshop on Machine Learning for Signal Processing (MLSP)}, title={Voice Anonymization in Urban Sound Recordings},
year={2019},
volume={},
