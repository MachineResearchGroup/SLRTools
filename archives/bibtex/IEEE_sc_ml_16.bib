@INPROCEEDINGS{8855605,
author={Yu, Min and Jiang, Jianguo and Li, Gang and Li, Jingyuan and Lou, Chenzhe and Liu, Chao and Huang, Weiqing and Wang, Yuanzhuo},
booktitle={2019 IEEE 21st International Conference on High Performance Computing and Communications; IEEE 17th International Conference on Smart City; IEEE 5th International Conference on Data Science and Systems (HPCC/SmartCity/DSS)}, title={A Unified Malicious Documents Detection Model Based on Two Layers of Abstraction},
year={2019},
volume={},
number={},
pages={2317-2323},
abstract={Due to the ever increasing attacks using malicious documents, the detection of such documents has become a serious and urgent research issue. In the past decade, detection of malicious documents has attracted significant research attentions, and many methods have been proposed, including conventional static detection methods, and dynamic detection methods. However, both of the two categories of methods have limitations under either obfuscated or run-time conditions, and can not achieve a satisfactory detection performance for malicious behaviors. In this work, we firstly present a new descriptive structure of our targeted documents, using a two layers abstraction including the structure and the scripting language. We then propose a unified model for malicious documents detection based on two layers of abstraction. A series of experiments under a real world data set with 20,000 samples show that, our proposed model has a better detection performance for all of the four indicators, the accuracy, precision, recall, and AUC of malicious documents simultaneously, when compared to the Hidost model.},
keywords={Portable document format;Feature extraction;Malware;Tools;Machine learning algorithms;Containers;Malicious Documents Detection;Unified Detection Model;Abstraction;PDF Documents;Office Documents},
doi={10.1109/HPCC/SmartCity/DSS.2019.00322},
