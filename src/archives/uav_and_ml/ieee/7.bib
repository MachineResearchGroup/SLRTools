booktitle={2021 Latin American Robotics Symposium (LARS), 2021 Brazilian Symposium on Robotics (SBR), and 2021 Workshop on Robotics in Education (WRE)}, title={Autonomous and Collective Intelligence for UAV Swarm in Target Search Scenario},
year={2021},
