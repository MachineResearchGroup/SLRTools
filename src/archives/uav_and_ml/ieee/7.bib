@INPROCEEDINGS{9016432,
author={Kim, Kitae and Park, Yu Min and Seon Hong, Choong},
booktitle={2020 International Conference on Information Networking (ICOIN)}, title={Machine Learning Based Edge-Assisted UAV Computation Offloading for Data Analyzing},
year={2020},
