month={June},}
@INPROCEEDINGS{9448868,
author={Zhu, Shichao and Gui, Lin and Zhang, Qi and Lang, Xiupu},
booktitle={2021 IEEE 93rd Vehicular Technology Conference (VTC2021-Spring)}, title={Joint Client Association and UAV Scheduling in Cache-Enabled UAV-Assisted Vehicular Networks},
year={2021},
volume={},
