@INPROCEEDINGS{9613503,
author={Ye, Haokeke and Liu, Jing},
booktitle={2021 13th International Conference on Wireless Communications and Signal Processing (WCSP)}, title={An Enhanced Q-Learning Routing Algorithm Based on Trajectory Prediction for UAV Networks},
year={2021},
volume={},
number={},
pages={1-5},
abstract={With the development of artificial intelligence technology, research has been showing an upward trend on intelligent Unmanned Aerial Vehicles(UAV). For highly dynamic UAV networks, the existing routing protocols have been considering the connected link between nodes into routing, but the exploring of mobility characteristics of UAV networks is not sufficient. In this paper, we propose an enhanced Q-Learning routing algorithm called PreQ-Learning with trajectory prediction of nodes being neighbors and pre-calculation of Q-value. This algorithm introduces a mechanism of calculating Q-value in advance before nodes meet, accelerating the convergence of Q-Learning algorithm, and making it more suitable for UAV networks. The simulation results confirm that the PreQ-Learning routing algorithm can obtain a higher Packet Delivery Ratio than OLSR and Q-Learning routing algorithm in UAV networks.},
keywords={Wireless communication;Simulation;Signal processing algorithms;Signal processing;Prediction algorithms;Autonomous aerial vehicles;Routing;UAV networks;Trajectory prediction;PreQ-Learning},
doi={10.1109/WCSP52459.2021.9613503},
ISSN={2472-7628},
month={Oct},}
@INPROCEEDINGS{9590400,
author={Zhang, Chong and Liu, Xiao and Xu, Jia and Chen, Tianxiang and Li, Gang and Jiang, Frank and Li, Xuejun},
booktitle={2021 IEEE International Conference on Web Services (ICWS)}, title={An Edge based Federated Learning Framework for Person Re-identification in UAV Delivery Service},
year={2021},
volume={},
number={},
pages={500-505},
abstract={AI (Artificial Intelligence) technology has been widely used in smart systems which usually require computing services with high availability and fast response. However, the rapid growth of data and service requests generated by end devices brings critical challenges to the centralised cloud computing paradigm in terms of network bandwidth, reliability and response time. In addition, the problem of data privacy is arising due to a large amount of data being transferred to the cloud server. Recently, edge computing is becoming a popular platform for smart systems as its provisions computing services close to the end devices, and Federated Learning (FL) is emerging as a promising solution for AI applications to address the data privacy issue. Inspired by their success, in this paper, we propose an edge based FL framework named Fed-UAV to solve the person reidentification problem in the UAV delivery service which is a typical AI application in smart logistics. This framework enables the UAV to efficiently locate the target receivers, and effectively reduce the data transmission between the UAV and the cloud server to improve the response time and protect the data privacy. Comprehensive experiments are conducted on three real-world datasets, and the experimental result successfully demonstrates that Fed-UAV can achieve both high accuracy and efficiency in person re-identification while protecting data privacy.},
keywords={Knowledge engineering;Data privacy;Cloud computing;Face recognition;Computational modeling;Receivers;Servers;Edge Intelligence;Edge Computing;Federated Learning;UAV Delivery;Person Re-identification},
doi={10.1109/ICWS53863.2021.00070},
ISSN={},
month={Sep.},}
@INPROCEEDINGS{9169032,
author={Liu, Guanghui and Zhang, Chunlin and Guo, Qing and Wan, Fangyi},
booktitle={2019 International Conference on Sensing, Diagnostics, Prognostics, and Control (SDPC)}, title={Automatic Color Recognition Technology of UAV Based on Machine Vision},
year={2019},
volume={},
number={},
pages={220-225},
abstract={With the rapid development of artificial intelligence technology, machine vision, as a branch of it, has been widely used in various fields. Among them, color recognition is an important application in machine vision. Based on the background of the international UAV innovation competition, this paper studies the real-time color direction light image collected by the UAV camera, and proposes an automatic color recognition model of UAV based on machine vision. In this paper, the characteristics of the directional signal light image acquired by UAV under different color space models and its influence on the color recognition effect are expounded, and the principle and conversion method of the HSV color space model are emphatically discussed. Through image edge detection, erosion and dilation and closed operation processing, the geometric features of the image of the directional signal light are extracted, thereby identifying the color of the directional signal light. Finally, the experiment of automatic recognition of the color of the airport signal lights shows that the proposed color recognition model can effectively detect the objects in complex background.},
keywords={Image color analysis;Image edge detection;Feature extraction;Machine vision;Real-time systems;Airports;image processing;machine vision;color space model;color recognition;Canny operator},
doi={10.1109/SDPC.2019.00047},
ISSN={},
