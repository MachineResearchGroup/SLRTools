author={Chie, Lee Chia and Juin, Yap Wen},
booktitle={2020 IEEE 7th International Conference on Industrial Engineering and Applications (ICIEA)}, title={Artificial Landmark-based Indoor Navigation System for an Autonomous Unmanned Aerial Vehicle},
year={2020},
volume={},
