@ARTICLE{9114970,
author={Cui, Jingjing and Ding, Zhiguo and Deng, Yansha and Nallanathan, Arumugam and Hanzo, Lajos},
journal={IEEE Access}, title={Adaptive UAV-Trajectory Optimization Under Quality of Service Constraints: A Model-Free Solution},
