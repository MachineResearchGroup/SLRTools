@INPROCEEDINGS{9653034,
author={Sherman, Michelle and Shao, Sihua and Sun, Xiang and Zheng, Jun},
booktitle={MILCOM 2021 - 2021 IEEE Military Communications Conference (MILCOM)}, title={UAV Assisted Cellular Networks With Renewable Energy Charging Infrastructure: A Reinforcement Learning Approach},
year={2021},
