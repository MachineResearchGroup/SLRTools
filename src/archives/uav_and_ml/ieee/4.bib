@ARTICLE{9020018,
author={Zhang, Xiaoming and Ali, Mohsin},
journal={IEEE Access}, title={A Bean Optimization-Based Cooperation Method for Target Searching by Swarm UAVs in Unknown Environments},
year={2020},
