@ARTICLE{9455141,
author={Zuo, Ming and Xie, Shuguo and Zhang, Xian and Yang, Meiling},
journal={IEEE Access}, title={Recognition of UAV Video Signal Using RF Fingerprints in the Presence of WiFi Interference},
