@INPROCEEDINGS{9071687,
author={Li, Hanwen and Yang, Zhenkai and Wang, Yun and Yao, Li and Zhao, Xiaojie},
booktitle={2019 IEEE International Smart Cities Conference (ISC2)}, title={Construction of Psychological Crisis Assessment Model Based on Machine Learning},
year={2019},
volume={},
