@ARTICLE{9153758,
author={Huynh-The, Thien and Hua, Cam-Hao and Tu, Nguyen Anh and Kim, Dong-Seong},
journal={IEEE Internet of Things Journal}, title={Physical Activity Recognition With Statistical-Deep Fusion Model Using Multiple Sensory Data for Smart Health},
