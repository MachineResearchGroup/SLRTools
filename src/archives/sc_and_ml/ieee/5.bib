@INPROCEEDINGS{8905160,
author={Duong, Ngoc-Son and Dinh, Thai-Mai},
booktitle={2019 19th International Symposium on Communications and Information Technologies (ISCIT)}, title={Indoor Localization with lightweight RSS Fingerprint using BLE iBeacon on iOS platform},
year={2019},
volume={},
