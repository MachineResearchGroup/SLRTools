month={Aug},}
@INPROCEEDINGS{9196314,
author={Thakur, Nirmalya and Han, Chia Y.},
booktitle={2020 International Conference on Big Data, Artificial Intelligence and Internet of Things Engineering (ICBAIE)}, title={A Framework for Prediction of Cramps during Activities of Daily Living in Elderly},
year={2020},
volume={},
