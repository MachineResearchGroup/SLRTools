@ARTICLE{8908558,
author={Chen, Ning and Qiu, Tie and Zhou, Xiaobo and Li, Keqiu and Atiquzzaman, Mohammed},
journal={IEEE Communications Magazine}, title={An Intelligent Robust Networking Mechanism for the Internet of Things},
