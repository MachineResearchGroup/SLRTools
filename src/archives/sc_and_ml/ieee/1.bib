@ARTICLE{9737410,
author={Yuan, Xiaoming and Chen, Jiahui and Yang, Jiayu and Zhang, Ning and Yang, Tingting and Han, Tao and Taherkordi, Amir},
journal={IEEE Transactions on Intelligent Transportation Systems}, title={FedSTN: Graph Representation Driven Federated Learning for Edge Computing Enabled Urban Traffic Flow Prediction},
year={2022},
volume={},
